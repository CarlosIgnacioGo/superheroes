<h1 class="title">I am Batman</h1>
<%= form_tag pages_create_path, method: :post,heroe: "batman" %>
    <%= label_tag "email", "Email" %>
    <%= email_field_tag "email" %>
    <%= hidden_field_tag(:heroe, value = "batman") %>
	<%= submit_tag "Votar" %>
<script>
	 $.backstretch("<%= asset_path 'batman.jpg' %>");
</script>
